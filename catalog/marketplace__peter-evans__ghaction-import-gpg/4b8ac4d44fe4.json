{
  "action_id": "marketplace/peter-evans/ghaction-import-gpg",
  "version": "4b8ac4d44fe4",
  "definition": {
    "name": "Import GPG",
    "description": "GitHub Action to easily import a GPG key",
    "author": "crazy-max",
    "inputs": [
      {
        "name": "gpg_private_key",
        "description": "GPG private key exported as an ASCII armored version or its base64 encoding",
        "required": true,
        "default": null
      },
      {
        "name": "passphrase",
        "description": "Passphrase of the GPG private key",
        "required": false,
        "default": null
      },
      {
        "name": "trust_level",
        "description": "Set key's trust level",
        "required": false,
        "default": null
      },
      {
        "name": "git_config_global",
        "description": "Set Git config global",
        "required": false,
        "default": "false"
      },
      {
        "name": "git_user_signingkey",
        "description": "Set GPG signing keyID for this Git repository",
        "required": false,
        "default": "false"
      },
      {
        "name": "git_commit_gpgsign",
        "description": "Sign all commits automatically",
        "required": false,
        "default": "false"
      },
      {
        "name": "git_tag_gpgsign",
        "description": "Sign all tags automatically",
        "required": false,
        "default": "false"
      },
      {
        "name": "git_push_gpgsign",
        "description": "Sign all pushes automatically",
        "required": false,
        "default": "if-asked"
      },
      {
        "name": "git_committer_name",
        "description": "Commit author's name",
        "required": false,
        "default": null
      },
      {
        "name": "git_committer_email",
        "description": "Commit author's email",
        "required": false,
        "default": null
      },
      {
        "name": "workdir",
        "description": "Working directory (below repository root)",
        "required": false,
        "default": "."
      },
      {
        "name": "fingerprint",
        "description": "Specific fingerprint to use (subkey)",
        "required": false,
        "default": null
      }
    ],
    "outputs": [
      {
        "name": "fingerprint",
        "description": "Fingerprint of the GPG key (recommended as user ID)"
      },
      {
        "name": "keyid",
        "description": "Low 64 bits of the X.509 certificate SHA-1 fingerprint"
      },
      {
        "name": "name",
        "description": "Name associated with the GPG key"
      },
      {
        "name": "email",
        "description": "Email address associated with the GPG key"
      }
    ]
  },
  "source": {
    "type": "marketplace",
    "publisher": "peter-evans",
    "verified": false,
    "origin": "github.com/peter-evans/ghaction-import-gpg"
  },
