  "definition": {
    "name": "GitHub Pull Request Action",
    "description": "\u2935\ufe0f Create pull request",
    "author": "Wei He <github@weispot.com>",
    "inputs": [
      {
        "name": "destination_repository",
        "required": false,
        "default": null,
