{
  "action_id": "marketplace/peter-evans/create-issue-from-file",
  "version": "f89650e9d2a8",
  "definition": {
    "name": "Create Issue From File",
    "description": "An action to create an issue using content from a file",
    "author": "peter-evans",
    "inputs": [
      {
        "name": "token",
        "description": "The GitHub authentication token",
        "required": false,
        "default": "${{ github.token }}"
      },
      {
        "name": "repository",
        "description": "The target GitHub repository",
        "required": false,
        "default": "${{ github.repository }}"
      },
      {
        "name": "issue-number",
        "description": "The issue number of an existing issue to update",
        "required": false,
        "default": null
      },
      {
        "name": "title",
        "description": "The title of the issue",
        "required": true,
        "default": null
      },
      {
        "name": "content-filepath",
        "description": "The file path to the issue content",
        "required": false,
        "default": null
      },
      {
        "name": "labels",
        "description": "A comma or newline-separated list of labels",
        "required": false,
        "default": null
      },
      {
        "name": "assignees",
        "description": "A comma or newline-separated list of assignees (GitHub usernames)",
        "required": false,
        "default": null
      }
    ],
    "outputs": [
      {
        "name": "issue-number",
        "description": "The number of the created issue"
      }
    ]
  },
